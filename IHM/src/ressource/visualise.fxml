<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.web.*?>
<?import javafx.scene.text.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>

<BorderPane prefHeight="500.0" prefWidth="950.0" xmlns="http://javafx.com/javafx/8"
   xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.VisualiseController">
   <top>
      <AnchorPane prefHeight="60.0" prefWidth="600.0" BorderPane.alignment="CENTER">
         <children>
            <ImageView fx:id="flagHome" fitHeight="63.0" fitWidth="90.0" layoutX="-9.0"
               pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@data/img/flag.png" />
               </image>
            </ImageView>
            <TextField fx:id="citySearchField" layoutX="752.0" layoutY="19.0" />
            <Button layoutX="908.0" layoutY="19.0" mnemonicParsing="false"
               onAction="#launchResearchData" style="-fx-background-color: null;" text="🔍" />
         </children>
      </AnchorPane>
   </top>
   <center>
      <ScrollPane prefHeight="1274.0" prefWidth="950.0" BorderPane.alignment="CENTER">
         <content>
            <VBox fx:id="vBoxContainer" prefHeight="1026.0" prefWidth="935.0">
               <children>
                  <Pane prefHeight="166.0" prefWidth="935.0">
                     <children>
                        <Label layoutX="400.0" layoutY="81.0" prefHeight="44.0" prefWidth="136.0"
                           text="Visualiser">
                           <font>
                              <Font name="Copperplate Gothic Light" size="22.0" />
                           </font>
                        </Label>
                        <TextField fx:id="dataSearchField" layoutX="213.0" layoutY="133.0"
                           prefHeight="25.0" prefWidth="509.0"
                           promptText="Rechercher un type de données" />
                        <Label fx:id="cityError" layoutX="16.0" layoutY="31.0" prefHeight="11.0"
                           prefWidth="903.0" text="Label" textFill="RED" />
                        <Label fx:id="dataError" layoutX="17.0" layoutY="14.0" prefHeight="11.0"
                           prefWidth="903.0" text="Label" textFill="RED" />
                        <Button fx:id="adminButton" layoutX="382.0" layoutY="19.0"
                           mnemonicParsing="false" prefHeight="54.0" prefWidth="232.0"
                           text="Admin Button (Do NOT press)" />
                     </children>
                  </Pane>
                  <Pane fx:id="checkBoxPane" prefHeight="51.0" prefWidth="935.0">
                     <children>
                        <Separator layoutX="93.0" layoutY="39.0" prefHeight="11.0" prefWidth="749.0" />
                        <TextField fx:id="citySearchField" layoutX="213.0" layoutY="2.0"
                           prefHeight="25.0" prefWidth="509.0" promptText="Rechercher une ville" />
                     </children>
                  </Pane>
                  <HBox fx:id="dataBlock" alignment="CENTER" />
                  <HBox fx:id="cityBlock" alignment="CENTER" />
                  <Separator prefHeight="11.0" prefWidth="749.0" />
                  <Label fx:id="adminLabel" text="LABEL ADMIN" />
               </children>
            </VBox>
         </content>
      </ScrollPane>
   </center>
</BorderPane>